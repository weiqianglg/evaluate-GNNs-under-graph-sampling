{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "GAT",
    "dataset": "ogbl-collab",
    "largest_cc": true,
    "store_graphml": "",
    "cache": true,
    "out_dir": "out/COLLAB_edge_classification2",

    "sample": {
      "sample_method": "MetropolisHastingsRandomWalkSampler",
      "percent_of_nodes": 1,
      "seed": 41,
      "subgraph": false
    },
    
    "params": {
        "seed": 41,
        "epochs": 500,
        "batch_size": 32768,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-5,
        "weight_decay": 0.0,
        "num_epochs_patience": 20,
        "print_epoch_interval": 1,
        "max_time": 12
    },
    
    "net_params": {
        "L": 3,
        "hidden_dim": 19,
        "out_dim": 76,
        "residual": true,
        "readout": "mean",
        "n_heads": 4,
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "self_loop": false,
        "layer_type": "dgl"
    }
}